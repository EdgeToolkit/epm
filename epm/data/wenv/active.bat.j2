{% set name=config.wenv.name %}
{% set environment=config.environment %}
@ECHO OFF
if defined EPM_WORK_ENVIRONMENT (
  echo "Already in virtual environment %EPM_VIRTUAL_ENVIRONMENT%, you have to exits this venv, then try again."
  exit/b 1
)

set EPM_WORK_ENVIRONMENT={{name}}

epm wenv banner

title [epm wenv] - {{name}}
prompt $p [epm wenv]@{{name}} $_$$