{%- set name=config.venv.name %}
{%- set environment=config.environment %}
#!/bin/bash
__dir__=$(cd $(dirname ${BASH_SOURCE:-$0});pwd)
if [[ -n $EPM_VIRTUAL_ENVIRONMENT ]] ;; then
  echo "Already in virtual environment <$EPM_VIRTUAL_ENVIRONMENT>, you have to exits this venv, then try again."
  exit/b 1
fi


source ${__dir__}/bash.rc

epm venv banner